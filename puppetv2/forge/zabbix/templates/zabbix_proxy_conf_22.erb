# This file is managed by Puppet
Server=<%= zabbix_server %>
Hostname=<%= hostname %>
Include=<%= proxy_include_dir %>
LogFile=/var/log/zabbixsrv/zabbix_proxy.log
LogFileSize=100
ConfigFrequency=<%= zabbix_proxy_config_frequency %>
PidFile=/var/run/zabbixsrv/zabbix_proxy.pid
DBName=/var/lib/zabbixsrv/db/zabbix_proxy.db
DBUser=zabbix
DebugLevel=3
ProxyOfflineBuffer=24
StartPollers=10
StartTrappers=10
StartPingers=10
StartVMwareCollectors=10
VMwareFrequency=300
HistoryCacheSize=32M
HistoryTextCacheSize=64M
Timeout=5
ExternalScripts=/var/lib/zabbixsrv/externalscripts
TmpDir=/var/lib/zabbixsrv/tmp
