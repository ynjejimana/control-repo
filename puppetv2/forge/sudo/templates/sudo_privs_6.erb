Cmnd_Alias <%= name.upcase -%> = /usr/bin/ant, \
		/usr/sbin/apachectl, \
<% if @source_dir -%>
<% @source_dir.each do |sd| -%>
		/bin/cat <%= sd %>/*, \
		/usr/bin/chattr <%= sd %>/*, \
		/bin/chmod octal <%= sd %>/*, \
		/bin/chown user <%= sd %>/*, \
		/bin/gzip <%= sd -%>/*, \
		/bin/gzip -d <%= sd -%>/*, \
		/usr/bin/head <%= sd -%>/*, \
		/usr/bin/less <%= sd -%>/*, \
		/bin/mkdir <%= sd -%>/*, \
		/bin/more <%= sd -%>/*, \
		/bin/rm <%= sd -%>/*, \
		/bin/rm -f <%= sd -%>/*, \
		/bin/rm -r <%= sd -%>/*, \
		/bin/rm -rf <%= sd -%>/*, \
		/usr/bin/tail -f <%= sd -%>/*, \
		/bin/touch <%= sd -%>/*, \
		sudoedit <%= sd -%>/*, \
		/<%= sd -%>/*/orainstRoot.sh, \
		/<%= sd -%>/*/root.sh, \
		/<%= sd -%>/*/runInstaller, \
		/<%= sd -%>/*/sapinst, \
<% if @target_dir -%>
<% @target_dir.each do |td| -%>
		/bin/cp <%= sd -%>/* <%= td -%>/*, \
		/bin/cp -r <%= sd -%>/* <%= td -%>/*, \
		/usr/bin/ln -s <%= sd -%>/* <%= td -%>/*, \
		/bin/mv <%= sd -%>/* <%= td -%>/*, \
		/bin/tar cf <%= sd -%>/*.tar <%= td -%>/*, \
		/bin/tar cjf <%= sd -%>/*.tar.bz2 <%= td -%>/*, \
		/bin/tar czf <%= sd -%>/*.tar.bz2 <%= td -%>/*, \
		/bin/tar xf <%= sd -%>/*.tar <%= td -%>/*, \
		/bin/tar xjf <%= sd -%>/*.tar.bz2 <%= td -%>/*, \
		/bin/tar xzf <%= sd -%>/*.tar.gz <%= td -%>/*, \
<% end -%>
<% end -%>
<% end -%>
<% end -%>
<% if @target_file -%>
<% @target_file.each do |tf| -%>
		sudoedit <%= tf -%>, \
<% end -%>
<% end -%>
<% if @processes -%>
<% @processes.each do |p| -%>
		/usr/bin/pkill <%= p %>, \
<% end -%>
<% end -%>
<% if @services -%>
<% @services.each do |s| -%>
		/sbin/service <%= s -%> *, \
<% end -%>
<% end -%>
		/usr/bin/strace *, \
<% if @users -%>
<% @users.each do |u| -%>
		/bin/su - <%= u -%>, \
<% end -%>
<% end -%>
		/usr/bin/crontab -l *, \
		/usr/bin/du *, \
		/sbin/fuser *, \
		/usr/bin/ipcrm *, \
		/usr/bin/ipcs *, \
		/usr/bin/less /var/log/*, \
		/usr/bin/locate *, \
		/usr/sbin/lsof *, \
		/u*/app/oracle/product/*/*/root.sh, \
		/u*/app/oraInventory/orainstRoot.sh, \
		/sapmnt/*/exe/uc/linuxx86_64/cleanipc, \
		/usr/sap/*, \
		/*/sapinst, \
		/*/*/sapinst, \
		/etc/init.d/httpd start, \
		/etc/init.d/shibd start
%<%= privileged_group -%>	ALL=(root) <%= name.upcase %>
