[hosts]
hostentries=<%= hostname %>, puppet.esbu.lab.com.au, cpnrglobutil02, cpnrglobutil01

[interfaces]
BASE="eth0","eth1"


[pings]
standardpings = "cpnrglobutil02:10.9.35.11:cpnrglobutil02"

[mail]
servers = "cpnrglobutil02:10.9.35.11:tcp:25"
mailfrom = 'fake@harbourmsp.com'
rcptto = 'fake@harbourmsp.com'

[dns]
servers = "cpnrglobutil02:10.9.35.11:udp:53"
checkaddress = "puppet.esbu.lab.com.au", "cpnrglobutil02", "cpnrglobutil01"

[ldap]

[ntp]
#servers = "cpnrglobutil02:10.9.35.11:tcp:119"

[kernel]
checkparams=kernel.msgmnb, kernel.msgmax, kernel.shmall, fs.file-max, kernel.sem, kernel.shmmni, kernel.shmmax, net.core.rmem_default, net.core.rmem_max, net.core.wmem_default, net.core.wmem_max, fs.aio-max-nr, net.ipv4.ip_local_port_range

[perms]
u01="/u01", "oracle", "dba", "drwxrwxr-x"
stage="/stage", "oracle", "dba", "drwxrwxr-x"
appdata="/appdata", "root", "root", "drwxrwxrwx"
backup="/backup", "oracle", "dba", "drwxrwxr-x"

[packages]
pkgs=glibc-devel, glibc-headers, gcc, libgomp, libaio-devel, sysstat, unixODBC, unixODBC-devel, kernel-headers, binutils, elfutils-libelf, glibc, glibc-common, libaio, libgcc, make, elfutils-libelf-devel, elfutils-libelf-devel-static, telnet, rsync, lsof, wget, unzip, nfs-utils, screen, xterm, ethtool, bind-utils

[mounts]
nfs="/u01"
local="/boot","/"

[ports]
protocols=tcp

[processes]

[users]
siebel="54323","54324","/home/siebel","/bin/bash"
oracle="54321","54321","/home/oracle","/bin/bash"
