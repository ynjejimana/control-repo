admin-email=<%= sw_admin_email %>
ssl-set-org=<%= sw_ca_org %>
ssl-set-org-unit=<%= sw_ca_org_unit %>
ssl-set-city=<%= sw_ca_city %>
ssl-set-state=<%= sw_ca_state %>
ssl-set-country=<%= sw_ca_country_code %>
ssl-password=<%= sw_ca_cert_password %>
ssl-set-email=<%= sw_ca_cert_email %>
<% if @sw_configure_apache_ssl == true then -%>
ssl-config-sslvhost=Y
<% elsif @sw_configure_apache_ssl == false then -%>
ssl-config-sslvhost=N
<% end -%>
db-backend=postgresql
db-name=<%= sw_db_name %>
db-user=<%= sw_db_user %>
db-password=<%= sw_db_pass %>
db-host=<%= sw_db_server %>
db-port=5432
<% if @sw_enable_cobbler == true then -%>
enable-tftp=Y
<% elsif @sw_enable_cobbler == false then -%>
enable-tftp=N
<% end -%>
